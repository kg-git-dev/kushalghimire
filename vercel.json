{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        },
        {
            "source": "/auctions",
            "destination": "/auctions/auctionList"
        },
        {
            "source": "auctions/:address",
            "destination": "/auctions/show"
        },
        {
            "source": "campaigns/:address",
            "destination": "/campaigns/show"
        }
    ]
}